$link 'stdio\n'

.hello 'Hello World!'



@ add3 (I32 I32 I32 > I32) {
	#0 input  I32
	#1 input  I32
	#2 input  I32

	#3 iadd #0 #1
	#4 iadd #4 #2

	#5 output I32 #5
}



@ divmod (I32 I32 > I32 I32) {
	#0 input  I32
	#1 input  I32

	#2 div #0 #1
	#3 mod #0 #1

	#4 output I32 #2
	#5 output I32 #3
}



@ func (I32 I32 > F32 F32) {

	#0 input  I32
	#1 input  I32

	#2 #3 divmod #0 #1
	#4 cast F32 #2
	#5 case F32 #3

	#6 output F32 #4
	#7 output F32 #5

}



% Int32x4 1 {
	#0 element *4*3*I32
}



"This is a comment."



^ :printf (*Char > I32) {
	-externName 'printf'
}



~ !print (*Char > ) [| IO] {

	-effectWrite

	#0 input *Char

	#1 constPrc :printf
	#2 forcecall #1 #0
}
