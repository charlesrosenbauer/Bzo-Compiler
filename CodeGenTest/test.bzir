$link 'stdio\n'

.hello 'Hello World!'


@ add3 (I32 I32 I32 > I32) {
	#0 input  I32
	#1 input  I32
	#2 input  I32

	#3 iadd #0 #1
	#4 iadd #4 #2

	#5 output I32 #5
}



@ divmod (I32 I32 > I32 I32) {
	#0 input  I32
	#1 input  I32

	#2 idiv #0 #1
	#3 imod #0 #1

	#4 output I32 #2
	#5 output I32 #3
}



@ func (I32 I32 > F32 F32) {

	#0 input  I32
	#1 input  I32

	#2 #3 call divmod #0 #1
	#4 cast F32 #2
	#5 cast F32 #3

	#6 output F16 #4
	#7 output F64 #5
}



% Int32x4 1 {
	#0 element *4*3*I32
}



% IO 0 {
	-effectSRSW
}



"This is a comment."



^ :printf (*Char > I32) [| IO] {
	-externName 'printf'
}



~ !print (*Char > ) [| IO] {

	#0 input *Char

	#1 prcall :printf #0
}
