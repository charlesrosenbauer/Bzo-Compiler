.hello 'Hello World!'



@ add3 (I32 I32 I32 > I32) {
	#0 input  I32
	#1 input  I32
	#2 input  I32
	#3 output I32

	#4 iadd #0 #1
	#5 iadd #4 #2
	
	#6 return #3 #5
}



@ divmod (I32 I32 > I32 I32) {
	#0 input  I32
	#1 input  I32
	#2 output I32
	#3 output I32

	#4 div #0 #1
	#5 mod #0 #1

	#6 return #2 #4
	#7 return #3 #5
}



@ func (I32 I32 > F32 F32) {

	#0 input  I32
	#1 input  I32
	#2 output F32
	#3 output F32

	#4 #5 divmod #0 #1
	#6 cast F32 #4
	#7 case F32 #5

	#8 return #2 #6
	#9 return #3 #7

}



% Int32x4 1 {
	#0 element [4]I32
}



