^ :printf (*Char > I32) {
	#0 input *Char
	#1 output I32

	#2 ffiCall 'printf' #0
	
	#3 return #1 #2
}



~ !print (*Char > ) IO {

	-effectWrite

	#0 input *Char

	#1 forcecall :printf #0
}
