$link 'stdio\n'

.hello 'Hello World!'

@ add3 (I32 I32 I32 > I32) {
	#0 input  I32
	#1 input  I32
	#2 input  I32
	#3 output I32

	#4 iadd #0 #1
	#5 iadd #4 #2
	
	#6 return #3 #5
}

@ divmod (I32 I32 > I32 I32) {
	#0 input  I32
	#1 input  I32
	#2 output I32
	#3 output I32

	#4 div #0 #1
	#5 mod #0 #1

	#6 return #2 #4
	#7 return #3 #5
}

% Int32x4 1 {
	#0 element [4]I32
}

